{
  "name": "angular-board",
  "version": "7.2.0",
  "description": "Tutorial Angular",
  "homepage": "https://academia-binaria.com",
  "author": {
    "name": "Alberto Basalo",
    "email": "albertobasalo@agorabinaria.com",
    "url": "https://twitter.com/albertobasalo"
  },
  "scripts": {
    "build:doc": "cd ./markdown/ && bs e -o ../docs/readme",
    "build:prod": "ng build --prod",
    "build:pub": "ng build --prod --output-path docs --base-href https://academiabinaria.github.io/angular-board/",
    "e2e": "ng e2e",
    "http-server": "http-server ./dist/angular-board/ -c-1 -p4271 -a localhost -o",
    "lint": "ng lint",
    "ng": "ng",
    "pub:doc": "npm run build:doc && npm run push",
    "pub:pdf": "cd ./markdown/ && bs p -h -o ../public/readme",
    "pub": "npm run build:pub && npm run build:doc && npm run push",
    "push": "git add * && git commit -m 'pub' && git push",
    "start:doc": "cd ./markdown/ && bs s",
    "start:prod": "npm run build:prod && npm run http-server",
    "start": "ng serve --aot -o --port 4270",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.2",
    "@angular/common": "~7.2.2",
    "@angular/compiler": "~7.2.2",
    "@angular/core": "~7.2.2",
    "@angular/forms": "~7.2.2",
    "@angular/platform-browser": "~7.2.2",
    "@angular/platform-browser-dynamic": "~7.2.2",
    "@angular/router": "~7.2.2",
    "core-js": "^2.5.4",
    "mini.css": "^3.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "~7.2.2",
    "@angular/language-service": "~7.2.2",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4"
  }
}
